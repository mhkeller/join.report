nav
  position relative
  width calc(100% - 10px)
  height 20px
  margin 5px auto 0 auto
  color #000

#title,
#by
  position absolute
  top 0
  a
    text-decoration none
    font-weight bold
    color #000
    &:hover
      text-decoration underline
      cursor pointer
#title
  left 0
  font-weight bold
#by
  right 0


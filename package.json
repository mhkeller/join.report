{
  "name": "data.join",
  "version": "1.0.0",
  "description": "Join data",
  "main": "package.json",
  "scripts": {
    "deploy": "npm run build && git commit -am 'build-for-deploy' && git subtree push --prefix public origin gh-pages",
    "test": "standard src/js/**/*.js",
    "notify": "./build/notify-cli.js",
    "predev": "WSK_DEV=true npm run build --silent",
    "dev:js": "node build/tasks/rollup-watch.js & node build/tasks/js-watch.js",
    "dev:css": "node build/tasks/stylus-watch.js",
    "dev:html": "node build/tasks/html-watch.js",
    "dev:server": "node build/tasks/browser-sync.js",
    "dev:statics": "node build/tasks/statics-watch.js",
    "build:js": "node build/tasks/rollup.js",
    "build:css": "node build/tasks/stylus-build.js",
    "build:css-min": "npm run build:css -- false -c",
    "build:html": "node ./build/tasks/html-build.js",
    "build:statics": "node ./build/tasks/statics-build.js",
    "build-view": "npm run build --silent && npm run view --silent",
    "preflight": "node build/preflight.js",
    "config": "node build/config-prj.js",
    "view": "npm run dev:server",
    "build": "npm run build:statics --silent && npm run build:js --silent && npm run build:css-min --silent && npm run build:html --silent && npm run notify -- -m 'Your project is ready!' -d 'success'",
    "dev": "npm run dev:js --silent & npm run dev:html --silent & npm run dev:css --silent & npm run dev:statics --silent & npm run dev:server --silent"
  },
  "author": "Michael Keller",
  "license": "MIT",
  "devDependencies": {
    "archieml": "^0.3.1",
    "autoprefixer-stylus": "^0.9.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.11.2",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.3",
    "fs-extra": "^0.26.7",
    "glob": "^7.0.5",
    "html-minify": "<1.0.0",
    "indian-ocean": "^1.1.0",
    "inquirer": "^0.12.0",
    "inquirer-input-up": "^1.0.2",
    "minifyify": "^7.2.1",
    "mkdirp": "^0.5.1",
    "queue-async": "^1.2.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "rupture": "^0.6.1",
    "stylus": "<1.0.0",
    "uglify-js": "^2.8.12",
    "underscore": "^1.8.3",
    "underscore-template-pipe": "^1.0.0",
    "watchify": "^3.7.0",
    "wsk": "github:bizweekgraphics/wsk",
    "yargs": "^3.32.0"
  },
  "engines": {
    "node": "^4.4.3"
  },
  "dependencies": {
    "d3": "^4.7.4",
    "d3-dispatch": "^1.0.3",
    "d3-selection-multi": "^1.0.1",
    "indian-ocean": "github:mhkeller/indian-ocean#rollup",
    "joiner": "github:mhkeller/joiner#rollup",
    "shapefile": "^0.6.2",
    "underscore": "^1.8.3"
  }
}
